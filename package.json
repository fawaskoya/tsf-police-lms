{
  "name": "tsf-police-lms",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "clean": "rm -rf .next out node_modules/.cache",
    "clean:build": "npm run clean && npm run build",
    "clean:install": "rm -rf node_modules package-lock.json && npm install",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:seed": "tsx prisma/seed.ts",
    "db:studio": "prisma studio",
    "db:reset": "prisma migrate reset --force",
    "prepare": "husky install",
    "postinstall": "prisma generate",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "setup": "npm run clean:install && npm run db:generate && npm run db:push",
    "dev:setup": "npm run setup && npm run db:seed",
    "prod:build": "npm run clean && npm run type-check && npm run lint && npm run build",
    "error:check": "npm run type-check && npm run lint",
    "logs:errors": "grep -r \"console.error\\|throw\\|Error(\" . --include=\"*.ts\" --include=\"*.tsx\" --include=\"*.js\" --include=\"*.jsx\""
  },
